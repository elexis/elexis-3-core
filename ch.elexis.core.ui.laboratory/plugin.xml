               id="ch.elexis.labitem.edit.selected"
               name="Selected LabItem"
               optional="true"
               typeId="ch.elexis.PersistentObjectType">
         </commandParameter>
      </command>
      <command
            defaultHandler="ch.elexis.core.ui.laboratory.commands.CreateLabItemUi"
