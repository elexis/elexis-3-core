<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="ch.elexis.core.logging.feature"
      label="Elexis OpenSource Common Logging Feature"
      version="3.13.0.qualifier"
      provider-name="elexis.info">

   <description>
      Provides logback based logging for Elexis.
      Mehr Info unter http://wiki.elexis.info/ch.elexis.core.logging.feature.feature.group
   </description>

   <copyright url="http://elexis.info/de_contributers.html">
      Gerry Weirich, 2007-2013 und viele andere
   </copyright>

   <license url="http://www.eclipse.org/legal/epl-v10.html">
      Eclipse Public License v1.0
   </license>

   <requires>
      <import plugin="org.eclipse.osgi"/>
      <import plugin="org.eclipse.equinox.common"/>
      <import plugin="ch.qos.logback.classic" version="1.0.7" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="ch.qos.logback.core"
         version="0.0.0"/>

   <plugin
         id="ch.qos.logback.classic"
         version="0.0.0"/>

   <plugin
         id="org.eclipse.equinox.console"
         version="0.0.0"/>

   <plugin
         id="org.apache.felix.gogo.command"
         version="0.0.0"/>

   <plugin
         id="org.apache.felix.gogo.runtime"
         version="0.0.0"/>

   <plugin
         id="org.apache.felix.gogo.shell"
         version="0.0.0"/>

   <plugin
         id="ch.elexis.core.logging.default_configuration"
         version="0.0.0"/>

   <plugin
         id="ch.elexis.core.logging.osgi"
         version="0.0.0"/>

   <plugin
         id="slf4j.api"
         version="0.0.0"/>

   <plugin
         id="slf4j.osgi"
         version="0.0.0"/>

   <plugin
         id="jcl.over.slf4j"
         version="0.0.0"/>

   <plugin
         id="jul.to.slf4j"
         version="0.0.0"/>

</feature>
